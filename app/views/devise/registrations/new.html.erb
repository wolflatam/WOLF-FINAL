
<html>
<head>
	<title>WOLF-REGISTRO</title>	
</head>

<body>

	<div class="container marketing" style="padding-top: 80px">
		<div class="row" style="padding-bottom: 30px;";>

        <h2 style="padding-left: 50px">Inscr√≠bete en WOLF</h2>

				
			  <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
			  <%= devise_error_messages! %>

			  <div style="padding-left: 50px"><%= f.label :name %><br />
			  <%= f.text_field :name, :autofocus => true %></div>

			  <div style="padding-left: 50px"><%= f.label :last_name %><br />
			  <%= f.text_field :last_name, :autofocus => true %></div>

			  <div style="padding-left: 50px"><%= f.label :email %><br />
			  <%= f.email_field :email, :autofocus => true %></div>

			  <div style="padding-left: 50px"><%= f.label :password %><br />
			  <%= f.password_field :password %></div>

			  <div style="padding-left: 50px"><%= f.label :password_confirmation %><br />
			  <%= f.password_field :password_confirmation %></div>

			  <br>

			  <div style="padding-left: 50px"><%= f.submit "Sign up" %></div>
				<% end %>

				<br>
				
				<%= render "devise/shared/links" %>

		</div>
	</div><!-- /.row -->


</body>
</html>

